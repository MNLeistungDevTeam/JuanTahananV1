{
  "version": "1.0",
  "defaultProvider": "filesystem",
  "libraries": [
    {
      "library": "node_modules/devextreme/dist/css/icons/",
      "destination": "wwwroot/css/DevExpress/icons",
      "files": [
        "dxiconsmaterial.ttf",
        "dxiconsmaterial.woff",
        "dxiconsmaterial.woff2",
        "dxicons.ttf",
        "dxicons.woff2",
        "dxicons.woff"
      ]
    },
    {
      "library": "node_modules/bootstrap-fileinput/img/",
      "destination": "wwwroot/img",
      "files": [
        "loading-sm.gif",
        "loading.gif"
      ]
    },
    {
      "library": "node_modules/bootstrap-icons/font/fonts/",
      "destination": "wwwroot/css/fonts",
      "files": [
        "bootstrap-icons.woff",
        "bootstrap-icons.woff2"
      ]
    },
    {
      "library": "node_modules/jquery-treegrid/img/",
      "destination": "wwwroot/img",
      "files": [
        "collapse.png",
        "expand.png",
        "file.png",
        "folder.png"
      ]
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/lib/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.js.map",
        "dist/browser/signalr.min.js",
        "dist/browser/signalr.min.js.map"
      ]
    },
    {
      "provider": "unpkg",
      "library": "izitoast@1.4.0",
      "destination": "wwwroot/lib/izitoast/"
    },
    {
      "provider": "unpkg",
      "library": "intl-tel-input@21.1.4",
      "destination": "wwwroot/lib/intl-tel-input/"
    },
    {
      "provider": "unpkg",
      "library": "pdfjs-dist@4.2.67",
      "destination": "wwwroot/lib/pdfjs-dist/"
    },
    {
      "provider": "unpkg",
      "library": "bootstrap-fileinput@5.5.4",
      "destination": "wwwroot/lib/bootstrap-fileinput/"
    },
    {
      "provider": "unpkg",
      "library": "dropzone@6.0.0-beta.2",
      "destination": "wwwroot/lib/dropzone/"
    },
    {
      "provider": "unpkg",
      "library": "bootstrap-icons@1.11.3",
      "destination": "wwwroot/lib/bootstrap-icons/"
    }
  ]
}