                <button id="btnPrint" type="button" class="btn btn-xs waves-effect border-0">
                    <i class="fe-printer"></i> Print All
                </button>
                <button type="button" class="btn btn-tool border-0" data-bs-toggle="modal" data-bs-target="#attachmentsModal">
